{
  "name": "serverless-vscode",
  "displayName": "fal-serverless vscode plugin",
  "description": "The fal-serverless extension with @isolated and Python cloud execution support",
  "publisher": "fal-ai",
  "version": "0.0.1",
  "preview": true,
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "keywords": [
    "python",
    "fal-serverless",
    "isolated",
    "serverless",
    "fal.ai"
  ],
  "activationEvents": [
    "onLanguage:python"
  ],
  "contributes": {
    "configuration": {
      "title": "fal-serverless",
      "properties": {
        "falServerless.extensionEnv": {
          "type": "string",
          "description": "The environment to use for the extension"
        }
      }
    },
    "commands": [
      {
        "command": "falServerless.run",
        "title": "Run fal-serverless function",
        "arguments": [
          {
            "name": "filename",
            "description": "The path of the file"
          },
          {
            "name": "isolatedLine",
            "description": "The line of the isolated decorator"
          }
        ]
      }
    ]
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "prettier": "prettier --write \"./**/*.{js,json,ts}\""
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "organize-imports-cli": "^0.10.0",
    "prettier": "^2.8.7",
    "typescript": "^4.9.5"
  },
  "extensionDependencies": [
    "ms-python.python"
  ]
}
